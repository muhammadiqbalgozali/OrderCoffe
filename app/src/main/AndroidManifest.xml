<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.coffeorder">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_kopi"
        android:label="Kopi Kolonel 404"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".About"></activity>
        <activity android:name=".KomposisiCoffe" />
        <activity android:name=".DetailPesanan" />
        <activity android:name=".DataMenu" />
        <activity android:name=".PilihanMenuCoffe.MenuDrinks" />
        <activity android:name=".PilihanMenuCoffe.MenuFood" />
        <activity android:name=".PilihanMenuCoffe.MenuKopi" />
        <activity android:name=".PilihanMenu" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>